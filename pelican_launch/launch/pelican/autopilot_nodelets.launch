<launch>

  #### Nodelet manager ####################################

  <node pkg="nodelet" type="nodelet" name="pelican_manager" args="manager" output="screen" />

  #### Asctec Autopilot ###################################

  <node pkg="nodelet" type="nodelet" name="AutoPilotNodelet" args="load asctec_autopilot/AutoPilotNodelet pelican_manager" output="screen">
    <param name="port" type="string" value="/dev/ttyUSB0"/>
    <param name="speed" type="int" value="57600"/>

    <param name="enable_IMU_CALCDATA" type="bool" value="true"/>
    <param name="interval_IMU_CALCDATA" type="int" value="1"/>
    <param name="offset_IMU_CALCDATA" type="int" value="0"/>
  </node>

  #### Asctec Proc ########################################

  <node pkg="nodelet" type="nodelet" name="AsctecProcNodelet" args="load asctec_proc/AsctecProcNodelet pelican_manager" output="screen">

  </node>

</launch>



